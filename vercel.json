{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/Produk",
      "dest": "/Produk.html"
    },
    {
      "src": "/Kontak",
      "dest": "/Kontak.html"
    },
    {
      "src": "/pembayaran",
      "dest": "/pembayaran.html"
    },
    {
      "src": "/Promosi",
      "dest": "/Promosi.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}